		zone: landuse=orchard OR natural=scrub OR @isOneOf(natural,scree,bare_rock) OR @isOneOf(landuse,brownfield,greenfield,construction) OR @isOneOf(natural,mud,wetland) OR natural=glacier OR landuse=landfill OR landuse=quarry

		zone militaire: @isOneOf(military, danger_area, naval_base, range) OR landuse=military
		lac: natural=water OR @isOneOf(waterway,riverbank,dock) OR (@isOneOf(landuse,reservoir,basin) AND NOT covered=yes)
