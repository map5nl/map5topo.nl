		terrain: (@isOneOf(landuse,farm,farmland,allotments,greenhouse_horticulture,plant_nursery) OR landuse=forest OR natural=wood OR landuse=meadow OR @isOneOf(landuse,residential,retail,commercial,farmyard,s,industrial) OR landuse=railway OR landuse=vineyard OR @isOneOf(natural,fell,grassland) OR natural=heath OR @isOneOf(natural,sand,beach) OR @isOneOf(landuse,grass,village_green,recreation_ground) OR leisure=garden OR landuse=salt_pond OR (@isOneOf(leisure,golf_course,park))) AND NOT type=site
